<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="base">

    <xacro:include filename="$(find summit_xl_description)/robots/summit_xls.urdf.xacro"/>
    
    <xacro:arg name="base_prefix" default="right"/>
    <xacro:arg name="ros_planar_move_plugin" default="false"/>
    
    <xacro:summit_xl

        prefix="$(arg base_prefix)"
        publish_bf="true"
        hq="true"
    >

    </xacro:summit_xl>

</robot>


  